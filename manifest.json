{
  "manifest_version": 2,
  "name": "BrazilJS 2012",
  "description": "Chrome Apps - BrazilJS",
  "version": "1.0",
  "minimum_chrome_version": "23",
  "app": {
    "background": {
      "scripts": ["js/main.js"]
    }
  },
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "storage", "serial"
  ]
}
